{
    "name": "brainlabsweb/laravel-razorpay",
    "description": "Integrating Razorpay with Laravel Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahesh",
            "email": "brainlabs2010@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-openssl": "*",
        "php": "^7.1.3",
        "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.*",
        "razorpay/razorpay": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Brainlabsweb\\Razorpay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brainlabsweb\\Razorpay\\RazorpayServiceProvider"
            ],
            "aliases": {
                "Razorpay": "Brainlabsweb\\Razorpay\\RazorpayFacade"
            }
        }
    }
}
